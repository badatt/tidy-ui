{
  "name": "@tidy-ui/stack",
  "version": "0.48.7",
  "readme": "README.md",
  "scripts": {
    "build": "yarn clean && rollup -c ../../rollup.config.js --environment PACKAGE_NAME:stack",
    "build:md": "markdown-include ../../.markdown.json",
    "clean": "rimraf -rf dist && rimraf -rf build",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "reset": "yarn clean && rimraf -rf node_modules",
    "vet": "yarn lint",
    "yalc:publish": "yarn build && yalc publish",
    "yalc:push": "yarn build && yalc push",
    "docs": "jsdoc -X -c ../../jsdoc.conf.json > jsdoc.json"
  },
  "dependencies": {
    "@tidy-ui/commons": "^0.48.7"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "styled-components": "^5.3.3"
  },
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/badatt/tidy-ui"
  },
  "author": {
    "name": "Balu Praveen Datty",
    "email": "dev.badatt@gmail.om",
    "url": "https://github.com/badatt"
  },
  "keywords": [
    "react",
    "styled-components",
    "css-in-js",
    "react-ui",
    "components",
    "ui",
    "ui-components",
    "stack"
  ],
  "description": "Stack, as the name says, can be used to stack any react component, plain html element or even just text nodes in an orderly fashion. Provide a divider component that separate each node with in the stack. It is provided with various styling options like alignment, wrap, gap etc.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "jsdoc.json",
    "README.md"
  ]
}
