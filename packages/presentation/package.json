{
  "name": "@tidy-ui/presentation",
  "version": "0.13.6",
  "readme": "README.md",
  "scripts": {
    "build": "yarn clean && rollup -c ../../rollup.config.js --environment PACKAGE_NAME:presentation",
    "build:md": "markdown-include ../../.markdown.json",
    "clean": "rimraf -rf dist && rimraf -rf build",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "reset": "yarn clean && rimraf -rf node_modules",
    "vet": "yarn lint",
    "yalc:publish": "yarn build && yalc publish",
    "yalc:push": "yarn build && yalc push"
  },
  "dependencies": {
    "@tidy-ui/badge": "^0.13.6",
    "@tidy-ui/button": "^0.13.6",
    "@tidy-ui/card": "^0.13.6",
    "@tidy-ui/message": "^0.13.6",
    "@tidy-ui/panel": "^0.13.6",
    "@tidy-ui/paper": "^0.13.6",
    "@tidy-ui/tag": "^0.13.6",
    "@tidy-ui/text": "^0.13.6"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "styled-components": "^5.3.3"
  },
  "main": "dist/index.js",
  "typings": "dist/src/index.d.ts",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/badatt/tidy-ui"
  },
  "author": {
    "name": "Balu Praveen Datty",
    "email": "dev.badatt@gmail.om",
    "url": "https://github.com/badatt"
  },
  "keywords": [
    "react",
    "styled-components",
    "css-in-js",
    "react-ui",
    "components",
    "ui",
    "ui-components",
    "badge",
    "anchor",
    "button",
    "button-group",
    "button-toolbar",
    "icon-button",
    "card",
    "message",
    "panel",
    "paper",
    "tag",
    "text"
  ],
  "description": "Tidy UI presentation library; 'presentation' package",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
