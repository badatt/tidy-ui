    "@tidy-ui/button": "^0.48.14",
    "@tidy-ui/card": "^0.48.14",
    "@tidy-ui/chip": "^0.48.14",
    "@tidy-ui/code": "^0.48.14",
    "@tidy-ui/message": "^0.48.14",
    "@tidy-ui/note": "^0.48.14",
    "@tidy-ui/panel": "^0.48.14",
    "@tidy-ui/paper": "^0.48.14",
    "@tidy-ui/table": "^0.48.14",
    "@tidy-ui/tag": "^0.48.14",
    "@tidy-ui/text": "^0.48.14",
    "@tidy-ui/tooltip": "^0.48.14"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "styled-components": "^5.3.3"
  },
  "module": "./dist/index.esm.js",
  "exports": {
    "import": "./dist/index.esm.js",
