{
  "name": "@tidy-ui/border",
  "version": "0.18.1",
  "readme": "README.md",
  "scripts": {
    "build": "yarn clean && rollup -c ../../rollup.config.js --environment PACKAGE_NAME:border",
    "build:md": "markdown-include ../../.markdown.json",
    "clean": "rimraf -rf dist && rimraf -rf build",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "reset": "yarn clean && rimraf -rf node_modules",
    "vet": "yarn lint",
    "yalc:publish": "yarn build && yalc publish",
    "yalc:push": "yarn build && yalc push"
  },
  "dependencies": {
    "@tidy-ui/commons": "^0.18.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "styled-components": "^5.3.3"
  },
  "main": "dist/index.js",
  "typings": "dist/src/index.d.ts",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/badatt/tidy-ui"
  },
  "author": {
    "name": "Balu Praveen Datty",
    "email": "dev.badatt@gmail.om",
    "url": "https://github.com/badatt"
  },
  "keywords": [
    "react",
    "styled-components",
    "css-in-js",
    "react-ui",
    "components",
    "ui",
    "ui-components",
    "border"
  ],
  "description": "Border component can be used to highlight border of a single component or a collection of them. You can add a descriptor inside the border at various positioning.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
