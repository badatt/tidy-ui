{
  "name": "@tidy-ui/checkbox",
  "version": "0.47.1",
  "readme": "README.md",
  "scripts": {
    "build": "yarn clean && rollup -c ../../rollup.config.js --environment PACKAGE_NAME:checkbox",
    "build:md": "markdown-include ../../.markdown.json",
    "clean": "rimraf -rf dist && rimraf -rf build",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "reset": "yarn clean && rimraf -rf node_modules",
    "vet": "yarn lint",
    "yalc:publish": "yarn build && yalc publish",
    "yalc:push": "yarn build && yalc push",
    "docs": "jsdoc -X -c ../../jsdoc.conf.json > jsdoc.json"
  },
  "dependencies": {
    "@tidy-ui/commons": "^0.47.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "styled-components": "^5.3.3"
  },
  "main": "dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "typings": "dist/src/index.d.ts",
  "types": "dist/src/index.d.ts",
  "browser": {
    "./dist/index.esm.js": "./dist/index.browser.esm.js",
    "./dist/index.cjs.js": "./dist/index.browser.cjs.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/badatt/tidy-ui"
  },
  "author": {
    "name": "Balu Praveen Datty",
    "email": "dev.badatt@gmail.om",
    "url": "https://github.com/badatt"
  },
  "keywords": [
    "react",
    "styled-components",
    "css-in-js",
    "react-ui",
    "components",
    "ui",
    "ui-components",
    "Checkbox"
  ],
  "description": "The Checkboxes component is a versatile tool for collecting user responses with a simple 'yes' or 'no.' Users can check for 'yes' and leave it for 'no.' It offers size and style options, including filled or outlined variants with tick marks upon selection. The intermediate state adds a line, and dynamic status reflects emitted information. Enhanced by robust theming and styling, it seamlessly adapts to your design preferences.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "jsdoc.json",
    "README.md"
  ]
}
