{
  "name": "@tidy-ui/drawer",
  "version": "0.48.14",
  "readme": "README.md",
  "type": "module",
  "scripts": {
    "build": "yarn clean && rollup -c ../../rollup.config.js --environment PACKAGE_NAME:drawer",
    "build:md": "markdown-include ../../.markdown.json",
    "clean": "rimraf -rf dist && rimraf -rf build",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "reset": "yarn clean && rimraf -rf node_modules",
    "vet": "yarn lint",
    "yalc:publish": "yarn build && yalc publish",
    "yalc:push": "yarn build && yalc push",
    "docs": "jsdoc -X -c ../../jsdoc.conf.json > jsdoc.json"
  },
  "dependencies": {
    "@tidy-ui/commons": "^0.48.14"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "styled-components": "^5.3.3"
  },
  "module": "./dist/index.esm.js",
  "exports": {
    "import": "./dist/index.esm.js",
    "default": "./dist/index.esm.js",
    "types": "./dist/src/index.d.ts"
  },
  "types": "./dist/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/badatt/tidy-ui"
  },
  "author": {
    "name": "Balu Praveen Datty",
    "email": "dev.badatt@gmail.om",
    "url": "https://github.com/badatt"
  },
  "keywords": [
    "react",
    "styled-components",
    "css-in-js",
    "react-ui",
    "components",
    "ui",
    "ui-components"
  ],
  "description": "The `Drawer` adds extra stuff, like navigation, to your app. You can put it on the left or right. Open/close it with clicks. When open, it covers the screen and focuses on its content. A top button closes it. Easy-peasy!",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "jsdoc.json",
    "README.md"
  ]
}
